{
    "name": "neoacevedo/laravel-gridview",
    "description": "Laravel GridView es un paquete para generar una tabla con datos de array de Arrays/Eloquent/Collections.",
    "type": "laravel-extension",
    "keywords": [
        "laravel",
        "html",
        "gridview",
        "table"
    ],
    "license": "GPL-3.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "24.03.07-RC2",
    "authors": [
        {
            "name": "NÃ©stor Acevedo",
            "email": "nestor.acevedo.romero@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "neoacevedo\\gridview\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "neoacevedo\\gridview\\Providers\\GridViewServiceProvider"
            ]
        }
    }
}
